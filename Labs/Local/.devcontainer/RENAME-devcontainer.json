{
    "name": "Tensorflow GPU Jupyter",
    "image": "tensorflow/tensorflow:latest-gpu-jupyter",
    "runArgs": [
      "--gpus=all"
    ],
    "forwardPorts": [8888],
    "overrideCommand": false,
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter"
        ]
      }
    }
  }